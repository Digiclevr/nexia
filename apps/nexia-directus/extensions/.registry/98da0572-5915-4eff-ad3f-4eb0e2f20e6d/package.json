{
  "name": "@devix-tecnologia/directus-extension-timeline",
  "description": "Directus extension for viewing collections in timeline format",
  "version": "1.2.6",
  "type": "module",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle",
    "node",
    "npm",
    "library",
    "directus",
    "directus-extensions",
    "timeline"
  ],
  "license": "MIT",
  "author": "Devix Tecnologia",
  "contributors": [
    {
      "name": "Sidarta Veloso",
      "github": "https://github.com/sidartaveloso",
      "linkedin": "https://www.linkedin.com/in/sidartaveloso"
    },
    {
      "name": "Fernando Gatti",
      "github": "https://github.com/gattifernando",
      "linkedin": "https://www.linkedin.com/in/gattifernando/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devix-tecnologia/directus-extension-timeline"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist/",
    "package.json",
    "yarn.lock"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "layout",
        "name": "multitimeline-layout",
        "source": "src/multitimeline-layout/index.ts"
      },
      {
        "type": "layout",
        "name": "timeline-layout",
        "source": "src/timeline-layout/index.ts"
      }
    ],
    "host": "^10.6.3"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "add": "directus-extension add",
    "test": "node tests/index.js",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "compose:up": "docker-compose up --build",
    "compose:down": "docker-compose down",
    "compose:psql": "docker-compose exec --user postgres directus_postgres psql",
    "compose:bash": "docker-compose exec --user postgres directus_postgres bash",
    "semantic-release": "semantic-release",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.1.11",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.28.0",
    "prettier": "3.1.0",
    "semantic-release": "^22.0.5",
    "typescript": "^5.1.3",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@devix-tecnologia/timeline-vue": "^3.1.0",
    "vue-i18n": "^10.0.3",
    "vue-router": "^4.2.2",
    "vuedraggable": "^2.24.3"
  }
}
