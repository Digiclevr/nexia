# NEXIA Claude Code Agent - Development Configuration
PORT=7013
NODE_ENV=development
AGENT_ID=nexia-claude-dev
NEXIA_SUPERVISOR_URL=http://localhost:7014

# Development Database URLs (local fallback)
DATABASE_URL=postgresql://nexia:password@postgres-central.platform.svc.cluster.local:5432/nexia_development
REDIS_URL=redis://platform-pool-redis-master.platform.svc.cluster.local:6379

# Development Thresholds (more relaxed)
MEMORY_THRESHOLD=95
CPU_THRESHOLD=90
RESPONSE_TIME_THRESHOLD=10000

# Agent Configuration - Development Mode
AGENT_MODE=alert
LOG_LEVEL=debug

# Development Ecosystem URLs (mock/localhost only)
BLUEOCEAN_API_BASE=mock://localhost:8020
BUSINESS_AUTO_BASE=mock://localhost:8001
ONLYONEAPI_BASE=https://api.onlyoneapi.com

# Development Settings
ENABLE_MOCKS=true
MOCK_HEALTHY_SERVICES=true
ECOSYSTEM_CHECK_INTERVAL=60000
ALERT_RATE_LIMIT=300000