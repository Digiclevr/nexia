{
  "name": "nexia-mobile",
  "version": "1.0.0",
  "description": "NEXIA Voice Mobile - Cross-platform mobile app for BlueOcean ecosystem",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios", 
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace NexiaMobile.xcworkspace -scheme NexiaMobile -configuration Release -destination 'generic/platform=iOS' -archivePath NexiaMobile.xcarchive archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "react-native-vector-icons": "^10.0.2",
    "react-native-audio-recorder-player": "^3.6.0",
    "react-native-sound-player": "^0.13.2",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "react-native-permissions": "^3.10.1",
    "react-native-websocket": "^1.0.2",
    "react-native-background-job": "^1.2.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.0",
    "@react-native/metro-config": "^0.72.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}